lder","buttonClass","buttonText","key","formData","read","default","delete","update","oldWord","newDef","post","word","definition","methods","handleSubmit","tabId","console","log","alert","toUpperCase","JSON","stringify"],"sources":["/var/home/Vin/vue_dictionary/vue_dictionary/src/components/VocabApp.vue"],"sourcesContent":["<template>\n  <div class=\"container mt-5\">\n    <ul class=\"nav nav-tabs mb-3\">\n      <li class=\"nav-item\" v-for=\"tab in tabs\" :key=\"tab.id\">\n        <button\n          class=\"nav-link\"\n          :class=\"{ active: activeTab === tab.id }\"\n          @click=\"activeTab = tab.id\"\n        >\n          {{ tab.name }}\n        </button>\n      </li>\n    </ul>\n\n    <div class=\"tab-content p-3 border border-top-0\">\n      <div\n        v-for=\"tab in tabs\"\n        :key=\"tab.id\"\n        v-show=\"activeTab === tab.id\"\n        class=\"tab-pane active\"\n      >\n        <form @submit.prevent=\"handleSubmit(tab.id)\">\n          <div\n            v-for=\"(field, index) in tab.fields\"\n            :key=\"index\"\n            class=\"mb-3\"\n          >\n            <label :for=\"tab.id + '-' + (field.key || 'default')\" class=\"form-label\">\n              {{ field.label }}\n            </label>\n            <input\n              type=\"text\"\n              class=\"form-control\"\n              :id=\"tab.id + '-' + (field.key || 'default')\"\n              v-model=\"formData[tab.id][field.key || 'default']\"\n              :placeholder=\"field.placeholder\"\n            />\n          </div>\n\n          <button type=\"submit\" class=\"btn\" :class=\"tab.buttonClass\">\n            {{ tab.buttonText }}\n          </button>\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"VocabApp\",\n  data() {\n    return {\n      activeTab: \"read\",\n      tabs: [\n        {\n          id: \"read\",\n          name: \"Read\",\n          fields: [\n            { label: \"Look up\", placeholder: \"Enter word...\" }\n          ],\n          buttonClass: \"btn-primary\",\n          buttonText: \"Search\"\n        },\n        {\n          id: \"delete\",\n          name: \"Delete\",\n          fields: [\n            { label: \"Word to delete\", placeholder: \"Enter word...\" }\n          ],\n          buttonClass: \"btn-danger\",\n          buttonText: \"Delete\"\n        },\n        {\n          id: \"update\",\n          name: \"Update\",\n          fields: [\n            { key: \"en\", label: \"Enlish\", placeholder: \"Current word...\" },\n            { key: \"th\", label: \"Thai\", placeholder: \"New meaning...\" }\n          ],\n          buttonClass: \"btn-warning\",\n          buttonText: \"Update\"\n        },\n        {\n          id: \"post\",\n          name: \"Post\",\n          fields: [\n            { key: \"en\", label: \"Enlish\", placeholder: \"Word...\" },\n            { key: \"th\", label: \"Thai\", placeholder: \"Meaning...\" }\n          ],\n          buttonClass: \"btn-success\",\n          buttonText: \"Add Word\"\n        }\n      ],\n      formData: {\n        read: { default: \"\" },\n        delete: { default: \"\" },\n        update: { oldWord: \"\", newDef: \"\" },\n        post: { word: \"\", definition: \"\" }\n      }\n    };\n  },\n  methods: {\n    handleSubmit(tabId) {\n      const data = this.formData[tabId];\n      console.log(`Form submitted for ${tabId}:`, data);\n      alert(`Submitted for ${tabId.toUpperCase()}:\\n${JSON.stringify(data, null, 2)}`);\n    }\n  }\n};\n</script>\n\n<style>\nbody {\n  background-color: #f8f9fa;\n}\n</style>\n"],"mappings":"AAiDA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,MAAM;MACjBC,IAAI,EAAE,CACJ;QACEC,EAAE,EAAE,MAAM;QACVJ,IAAI,EAAE,MAAM;QACZK,MAAM,EAAE,CACN;UAAEC,KAAK,EAAE,SAAS;UAAEC,WAAW,EAAE;QAAgB,EAClD;QACDC,WAAW,EAAE,aAAa;QAC1BC,UAAU,EAAE;MACd,CAAC,EACD;QACEL,EAAE,EAAE,QAAQ;QACZJ,IAAI,EAAE,QAAQ;QACdK,MAAM,EAAE,CACN;UAAEC,KAAK,EAAE,gBAAgB;UAAE